module StorageModel
{
    class StorageFile
    {
        String mime_type
        String data
    }

    class StorageFileAdd
    {
        String mime_type
        String file
    }

    class StorageFileDelete
    {
        String uri
    }

    class StorageFileAddress
    {
        String uri
    }

    class StorageFileRequest
    {
        String uri
        String authorization
    }

    class StorageFileRangeRequest
    {
        String uri
        String authorization
        UInt64 start
        UInt64 count
    }

    class StorageFileRange
    {
        String mime_type
        String data
        UInt64 full_size
        UInt64 start
        UInt64 count
    }

    class StorageFileDetails
    {
        String uri
    }

    class StorageFileDetailsResponse
    {
        String uri
        String mime_type
        UInt64 size
    }

    class FileUrisRequest {}

    class FileUris
    {
        Array String file_uris
    }

    class Done {}

    enum UriProblemType { missing duplicate invalid }
    class UriError
    {
        String uri
        UriProblemType uri_problem_type
    }

    class RemoteError { String message }
}
